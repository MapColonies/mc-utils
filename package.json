{
  "name": "@map-colonies/mc-utils",
  "version": "3.5.6",
  "description": "This is template for map colonies typescript packages",
  "main": "./dist/index.js",
  "scripts": {
    "test:unit": "jest --config=./tests/configurations/unit/jest.config.js",
    "test:integration": "jest --config=./tests/configurations/integration/jest.config.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint:fix": "npm run format:fix",
    "prelint": "npm run format",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "standard-version",
    "test": "npm run test:unit && npm run test:integration",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json",
    "start": "npm run build && cd dist && node ./index.js",
    "clean": "rimraf dist",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MapColonies/mc-utils.git"
  },
  "author": "MapColonies",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MapColonies/mc-utils/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/MapColonies/mc-utils#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@map-colonies/eslint-config": "^3.1.3",
    "@map-colonies/prettier-config": "0.0.1",
    "@types/express": "^4.17.17",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.171",
    "@types/ngeohash": "^0.6.3",
    "@types/node": "^14.14.12",
    "commitlint": "^17.4.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-create-mock-instance": "^2.0.0",
    "jest-html-reporters": "^3.1.4",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^9.1.1",
    "typedoc": "^0.22.18",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "axios": "^1.3.6",
    "axios-retry": "^3.4.0",
    "gdal-async": "^3.12.1",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "ngeohash": "^0.6.3",
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "@map-colonies/error-types": "^1.3.1",
    "@map-colonies/js-logger": "^1.0.1"
  },
  "engines": {
    "node": ">=20"
  },
  "//OverrideComment": "https://github.com/Turfjs/turf/issues/2922",
  "overrides": {
    "concaveman": "1.2.1"
  }
}
